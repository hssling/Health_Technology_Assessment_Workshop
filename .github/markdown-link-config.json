{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://github.com/hssling/Health_Technology_Assessment_Workshop/blob/main/"
    }
  ],
  "httpHeaders": [
    {
      "urls": [
        "https://github.com"
      ],
      "headers": {
        "Accept": "text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,*/*;q=0.8",
        "User-Agent": "Mozilla/5.0 (X11; Linux x86_64; rv:91.0) Gecko/20100101 Firefox/91.0"
      }
    }
  ],
  "timeout": "15s",
  "retryOn429": true,
  "retryCount": 3
}
